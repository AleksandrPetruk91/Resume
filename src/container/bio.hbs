<div>
  <main>
    <section>
      <div><h1>{{name}}</h1></div>
      <br />
      <p><b>Birthday :</b><time> {{birthdate}}.</time></p>
      <p><b>Place of birth:</b> {{birthplace}}. </p>
      <p><b>Date of death :</b>
        <time>{{deathdate}}.</time>
      </p>
      <p><b>Place of death:</b> {{deathplace}}. </p>
      <div><b>Nationality:</b> {{nationality}}</div>
      <br />
      <div><b>Occupation:</b> {{occupation}}</div>
      <br />
      <div><b>Known for:</b></div>
      {{#each known_for}}
        <ul>
          <li>{{this}}</li>
        </ul>
      {{/each}}
    </section>
    <hr />
    <section>
      <div><h2>Education</h2></div>

      {{#each education}}
        <ul>
          <li>Degree: {{degree}};</li>
          <li>Field: {{field}};</li>
          <li>Institution: {{institution}}</li>
          <li>Year : <time>{{year}}</time>;</li>
        </ul>
      {{/each}}
    </section>
    <hr />
    <section>
      <div><h3>Notable publications</h3></div>

      {{#each notable_publications}}
        <ul>
          <div><b>{{title}}</b></div>
          <br />
          <li>Year: <time>{{year}}</time></li>
          <li>Publisher: {{publisher}}</li>
        </ul>
      {{/each}}
    </section>
    <hr />
    <section>
      <div><h3>Partners</h3></div>

      {{#each partners}}
        <ul>
          <p>{{name}}
            -
            {{relationship}}, years
            <time>{{years}}</time>.</p>
        </ul>
      {{/each}}
    </section>
    <hr />
    <section>
      <div><h3>Awards</h3></div>

      {{#each awards}}
        <ul>
          <p>{{title}}.
            {{description}}
            in
            <time>{{year}}</time>.</p>
        </ul>
      {{/each}}
    </section>
    <hr />
    <section>
      <div><h3>Influences</h3></div>

      {{#each influences}}
        <ul>
          <li>{{this}}</li>
        </ul>
      {{/each}}
    </section>
    <hr />
    <section>
      <div><h3>Influenced:</h3></div>

      {{#each influenced}}
        <ul>
          <li>{{this}}np</li>
        </ul>
      {{/each}}
    </section>
    <hr />
    <section>
      <div><h3>Quotes</h3></div>

      {{#each quotes}}
        <ul>
          <li><blockquote>{{this}};</blockquote></li>
        </ul>
      {{/each}}
    </section>
    <hr />
    <section>
      <div><h3>Contributions</h3></div>

      {{#with contributions}}
        <ul>
          <div><b>{{title}}</b></div>
          <p>In <time>{{year}}</time> {{description}}.</p>
        </ul>
      {{/with}}
    </section>
  </main>
</div>